package in.ineuron.usingabstract;

abstract public class BankService {
	
	IciciBankServiceImpl iciciBankServiceImpl = new IciciBankServiceImpl();
	
	public abstract String checkBalance() {
		
	return	iciciBankServiceImpl.checkBalance();
		
	}
	
	public abstract String withdraw(float amount);
	public abstract String deposite(float amount);

}
